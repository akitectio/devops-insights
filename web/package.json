    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "axios": "^1.6.2",
    "concurrently": "^8.0.1",
    "formik": "^2.4.5",
    "i18next": "^23.7.9",
    "next": "^14.0.4",
    "next-i18next": "^15.1.1",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^13.5.0",
    "react-icons": "^4.12.0",
    "react-redux": "^9.0.4",
    "redux-saga": "^1.2.3",
